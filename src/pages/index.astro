---
import Layout from "../layouts/Layout.astro";
const posts = Object.values(import.meta.glob("./**/index.mdx", { eager: true })) as any[];
---

<Layout frontmatter={{ title: "Foo" }}>
  <p>Post Archive:</p>
  <ul>
    {
      posts.map((post) => (
        <li>
          <a href={post.url}>{post.frontmatter.title}</a>
        </li>
      ))
    }
  </ul>
</Layout>
